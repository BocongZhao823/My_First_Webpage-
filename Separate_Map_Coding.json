{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 300,
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/BocongZhao823/My_First_Webpage-/main/Ausmap.geo.json",
        "format": {"type": "topojson", "feature": "custom"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/BocongZhao823/My_First_Webpage-/main/AusNew.csv"
      },
      "mark": "circle",
      "encoding": {
        "longitude": {"field": "longitude", "type": "quantitative"},
        "latitude": {"field": "latitude", "type": "quantitative"},
        "size": {"value": 10},
        "color": {"value": "steelblue"}
      }
    }
  ],
  "config": {"projection": {"type": "orthographic", "rotate": [-140, 30, 0]}}
}